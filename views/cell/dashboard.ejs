  <!-- chart js -->
  <script>
    // Mock data representing the number of issues received
    const data = {
      day: {
        labels: ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday', 'Sunday'],
        counts: [5, 9, 3, 5, 2, 3, 7]
      },
      week: {
        labels: ['Week 1', 'Week 2', 'Week 3', 'Week 4'],
        counts: [23, 19, 32, 25]
      },
      month: {
        labels: ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October'],
        counts: [90, 100, 75, 80, 95, 110, 85, 90, 105, 120]
      }
    };

    let currentChart;

    // Function to initialize the chart
    function initChart(timeFrame) {
      const ctx = document.getElementById('issuesChart').getContext('2d');

      currentChart = new Chart(ctx, {
        type: 'bar', // You can change this to 'line', 'pie', etc.
        data: {
          labels: data[timeFrame].labels,
          datasets: [{
            label: `Number of Issues (${capitalizeFirstLetter(timeFrame)})`,
            data: data[timeFrame].counts,
            backgroundColor: 'rgba(54, 162, 235, 0.6)', // Blue bars
            borderColor: 'rgba(54, 162, 235, 1)',
            borderWidth: 1
          }]
        },
        options: {
          responsive: true,
          plugins: {
            legend: {
              labels: {
                color: 'blue' // Legend text color
              }
            },
            title: {
              display: true,
              text: `Citizen Issues - ${capitalizeFirstLetter(timeFrame)}`,
              color: 'blue'
            }
          },
          scales: {
            x: {
              grid: {
                color: 'rgba(54, 162, 235, 0.2)' // Light blue grid lines
              },
              ticks: {
                color: 'blue' // X-axis labels color
              }
            },
            y: {
              beginAtZero: true,
              ticks: {
                precision: 0,
                color: 'blue' // Y-axis labels color
              },
              grid: {
                color: 'rgba(54, 162, 235, 0.2)' // Light blue grid lines
              }
            }
          }
        }
      });
    }

    // Function to update the chart
    function updateChart(timeFrame) {
      if (currentChart) {
        currentChart.destroy();
      }
      initChart(timeFrame);
    }

    // Utility function to capitalize first letter
    function capitalizeFirstLetter(string) {
      return string.charAt(0).toUpperCase() + string.slice(1);
    }

    // Initialize the chart with default time frame
    window.onload = function () {
      initChart('day'); // Ensures daily data is displayed by default
    };



  </script>

